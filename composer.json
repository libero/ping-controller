{
    "name": "libero/ping-controller",
    "description": "Ping controller for a Libero API",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Libero\\PingController\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\Libero\\PingController\\": "tests/"
        }
    },
    "require": {
        "php": "^7.2",
        "psr/log": "^1.0",
        "symfony/http-foundation": "^3.4 || ^4.0"
    },
    "require-dev": {
        "libero/coding-standard": "^0.2",
        "phpstan/phpstan": "^0.10",
        "phpstan/phpstan-phpunit": "^0.10",
        "phpunit/phpunit": "^7.2",
        "symfony/debug": "^3.4 || ^4.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
